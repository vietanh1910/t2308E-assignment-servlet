<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>NFTList</servlet-name>
        <servlet-class>t2308e.assignment.controllers.NFTListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NFTList</servlet-name>
        <url-pattern>/nft-list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NFTCreate</servlet-name>
        <servlet-class>t2308e.assignment.controllers.NFTCreateController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NFTCreate</servlet-name>
        <url-pattern>/nft-create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NFTUpdate</servlet-name>
        <servlet-class>t2308e.assignment.controllers.NFTUpdateController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NFTUpdate</servlet-name>
        <url-pattern>/nft-update</url-pattern>
    </servlet-mapping>
</web-app>
